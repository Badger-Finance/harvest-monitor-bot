{
  "name": "harvest-monitor-bot",
  "version": "1.0.0",
  "description": "Discord bot to monitor Badger strategy harvests.",
  "main": "src/index.js",
  "author": "Ayush Shukla",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.6.0"
  },
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/providers": "^5.4.5",
    "ascii-table": "^0.0.9",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0-beta.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "prettier": "^2.4.0"
  },
  "scripts": {
    "start": "node --experimental-json-modules --require dotenv/config .",
    "deploy": "node --experimental-json-modules --require dotenv/config src/scripts/deploy-commands.js",
    "fetch": "node --experimental-json-modules --require dotenv/config src/scripts/fetch-strategy-metadata.js",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install"
  }
}
